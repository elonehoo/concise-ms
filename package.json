{
  "name": "@elonehoo/monorepo",
  "private": "true",
  "description": "understandable milliseconds",
  "author": "Elone Hoo <huchengyea@163.com>",
  "type": "module",
  "license": "MIT",
  "version": "0.0.2",
  "scripts": {
    "prepare": "pnpm -r run stub",
    "release": "bumpp package.json core/package.json --commit --push --tag && pnpm -r publish --access public",
    "typeCheck": "tsc --noEmit",
    "test": "pnpm -C test run test",
    "coverage": "pnpm -C test run coverage",
    "reporter": "pnpm -C test run reporter",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs-dev": "pnpm -C docs run dev",
    "docs-build": "pnpm -C docs run build"
  },
  "devDependencies": {
    "@elonehoo/eslint-config": "^2.0.0",
    "@types/node": "^17.0.43",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "pnpm": "^7.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3",
    "unbuild": "^0.7.4",
    "vite": "3.0.4"
  }
}
